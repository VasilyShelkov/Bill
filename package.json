{
  "name": "React-Hot-Dev-Starter",
  "version": "1.0.0",
  "description": "A skeleton project for starting with a hot-loading development react project server",
  "main": "index.html",
  "scripts": {
    "start": "npm run build && node prodServer.js",
    "build": "webpack --config webpack.config.prod.js --progress --profile --colors",
    "dev": "node devServer.js",
    "test": "NODE_ENV=test mocha './src/**/*.spec.js' --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VasilyShelkov/React-Hot-Dev-Starter.git"
  },
  "keywords": [
    "node",
    "react",
    "express",
    "hot-loading",
    "webpack-hot-middleware",
    "babel6",
    "eslint"
  ],
  "author": "Vasily Shelkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VasilyShelkov/React-Hot-Dev-Starter/issues"
  },
  "homepage": "https://github.com/VasilyShelkov/React-Hot-Dev-Starter#readme",
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "bootstrap": "^4.0.0-alpha.2",
    "express": "^4.13.4",
    "history": "^2.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "^0.9.6",
    "extend": "^3.0.0",
    "jsx-chai": "^4.0.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.2",
    "redbox-react": "^1.2.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
